  <!-- <script src="../../dist/js/adminlte.min.js"></script> -->
  <script href={Routes.static_path(@conn, "/dist/js/adminlte.min.js")}></script>
  <!-- AdminLTE for demo purposes -->
  <!-- <script src="../../dist/js/demo.js"></script> -->
  <script href={Routes.static_path(@conn, "/dist/js/demo.js")}></script>
  <!-- Phoenix Liveview -->
  <script href={Routes.static_path(@conn, "/js/app.js")}></script>
