<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title><%= @page_title %></title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <meta name="csrf-token" content={Plug.CSRFProtection.get_csrf_token()} />
  <!-- Bootstrap 3.3.7 -->
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/bower_components/bootstrap/dist/css/bootstrap.min.css")}>
  <!-- <link rel="stylesheet" href="../../bower_components/bootstrap/dist/css/bootstrap.min.css"> -->
  <!-- Font Awesome -->
  <link rel="stylesheet" href={Routes.static_path(@conn, "/bower_components/font-awesome/css/font-awesome.min.css")}>
  <!-- <link rel="stylesheet" href="../../bower_components/font-awesome/css/font-awesome.min.css"> -->
  <!-- Ionicons -->
  <link rel="stylesheet" href={Routes.static_path(@conn, "/bower_components/Ionicons/css/ionicons.min.css")}>
  <!-- <link rel="stylesheet" href="../../bower_components/Ionicons/css/ionicons.min.css"> -->
  <!-- daterange picker -->
  <!-- <link rel="stylesheet" href="../../bower_components/bootstrap-daterangepicker/daterangepicker.css"> -->
  <link rel="stylesheet" href={Routes.static_path(@conn, "/bower_components/bootstrap-daterangepicker/daterangepicker.css")}>
  <!-- bootstrap datepicker -->
  <!-- <link rel="stylesheet" href="../../bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css"> -->
  <link rel="stylesheet" href={Routes.static_path(@conn, "/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css")}>
  <!-- Select2 -->
  <!-- <link rel="stylesheet" href="../../bower_components/select2/dist/css/select2.min.css"> -->
  <link rel="stylesheet" href={Routes.static_path(@conn, "/bower_components/select2/dist/css/select2.min.css")}>
  <!-- Theme style -->
  <!-- <link rel="stylesheet" href="../../dist/css/AdminLTE.min.css"> -->
  <link rel="stylesheet" href={Routes.static_path(@conn, "/dist/css/AdminLTE.min.css")}>
  <link rel="stylesheet" href={Routes.static_path(@conn, "/dist/css/custom.css")}>
  <!-- AdminLTE Skins. Choose a skin from the css/skins
       folder instead of downloading all of them to reduce the load. -->
  <!-- <link rel="stylesheet" href="../../dist/css/skins/_all-skins.min.css"> -->
  <link rel="stylesheet" href={Routes.static_path(@conn, "/dist/css/skins/_all-skins.min.css")}>

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->

  <!-- temporary -->
  <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>

  <!-- Google Font -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-blue layout-fixed sidebar-mini">

  <!-- Site wrapper -->
  <div class="wrapper">
  <%= @inner_content %>
  </div>
  <!-- ./wrapper -->

  <!-- jQuery 3 -->
  <!-- <script src="../../bower_components/jquery/dist/jquery.min.js"></script> -->
  <script href={Routes.static_path(@conn, "/bower_components/jquery/dist/jquery.min.js")}></script>
  <!-- Bootstrap 3.3.7 -->
  <!-- <script src="../../bower_components/bootstrap/dist/js/bootstrap.min.js"></script> -->
  <script href={Routes.static_path(@conn, "/bower_components/bootstrap/dist/js/bootstrap.min.js")}></script>
  <!-- Select2 -->
  <!-- <script src="../../bower_components/select2/dist/js/select2.full.min.js"></script> -->
  <script href={Routes.static_path(@conn, "/bower_components/select2/dist/js/select2.full.min.js")}></script>
  <!-- date-range-picker -->
  <!-- <script src="../../bower_components/moment/min/moment.min.js"></script> -->
  <!-- <script src="../../bower_components/bootstrap-daterangepicker/daterangepicker.js"></script> -->
  <script href={Routes.static_path(@conn, "/bower_components/moment/min/moment.min.js")}></script>
  <script href={Routes.static_path(@conn, "/bower_components/bootstrap-daterangepicker/daterangepicker.js")}></script>
